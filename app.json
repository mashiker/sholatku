{
  "expo": {
    "name": "Sholatku - Jadwal Adzan, Kiblat",
    "slug": "sholatku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1628"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sholatku.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "SholatKu memerlukan akses lokasi untuk menghitung jadwal sholat dan arah kiblat.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "SholatKu memerlukan akses lokasi untuk notifikasi azan yang akurat.",
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a1628"
      },
      "package": "com.sholatku.app",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "SCHEDULE_EXACT_ALARM",
        "VIBRATE",
        "WAKE_LOCK",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow SholatKu to use your location."
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/adzan_subuh.mp3",
            "./assets/adzan_other.mp3"
          ]
        }
      ],
      "expo-font",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-8745470106514148~2314827593"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "32be22df-8b4b-4498-924d-1424e036a193"
      }
    }
  }
}